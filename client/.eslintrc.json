{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 10,
      "sourceType": "module"
    },
    "rules": {
        "brace-style": ["error", "allman"],
        "max-len": "off",
        "require-jsdoc": "off",
        "linebreak-style": "off",
        "quotes": ["error", "double"]
    }
}
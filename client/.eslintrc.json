{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "rules": {
        "brace-style": ["error", "allman"],
        "max-len": "off",
        "require-jsdoc": "off",
        "linebreak-style": "off",
        "quotes": ["error", "double"]
    },
    "parser": "babel-eslint"
}